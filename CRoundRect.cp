// CRoundRect.cp#include "CRoundRect.h"#include "CDrawUtils.h"#include "DrawConstants.h"CRoundRect::CRoundRect(LView* superCanvas, LWindow* superWindow)	: CShape(superCanvas, superWindow) {		}void CRoundRect::DrawMyShape(){	Rect shape;	CDrawUtils::CalcEnclosingRect(mStartPt, mEndPt, shape);	::PenNormal();		::RGBForeColor(&mFillColor);	::PaintRoundRect(&shape, kRoundRectRadius, kRoundRectRadius);		::RGBForeColor(&mFrameColor);	::FrameRoundRect(&shape, kRoundRectRadius, kRoundRectRadius);}int CRoundRect::GetClassInt() const { 	return msg_RoundRectTool; }